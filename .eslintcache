ming/ec.app/src/components/Products/SetSizeArea.jsx",[],"/Users/tamakikyou/proguraming/ec.app/src/reducks/products/actions.js",[],"/Users/tamakikyou/proguraming/ec.app/src/templates/ProductList.jsx",["176"],"import React, { useEffect } from \"react\";\nimport { ProductCard } from \"../components/Products\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { fetchProducts } from \"../reducks/products/operation\";\nimport { getProducts } from \"../reducks/products/selectors\";\n\nconst ProductList = () => {\n  const dispatch = useDispatch();\n  const selector = useSelector((state) => state);\n  const products = getProducts(selector);\n\n  useEffect(() => {\n    dispatch(fetchProducts());\n  }, []);\n\n\n  return (\n    <section className=\"c-section-wrapin\">\n      <div className=\"p-grid__row\">\n        {products.length > 0 &&\n          products.map((product) => <ProductCard key={product.key} id={product.id} name={product.name} images={product.images} price={product.price} />)}\n      </div>\n    </section>\n  );\n};\n\nexport default ProductList;\n","/Users/tamakikyou/proguraming/ec.app/src/reducks/products/selectors.js",[],"/Users/tamakikyou/proguraming/ec.app/src/components/Products/ProductCard.jsx",[],"/Users/tamakikyou/proguraming/ec.app/src/templates/ProductDetail.jsx",["177","178"],"import React, { useState, useEffect, useCallback } from \"react\";\nimport { useSelector } from \"react-redux\";\nimport { db } from \"../firebase/index\";\nimport { makeStyles } from \"@material-ui/core/styles\";\nimport HTMLReactParser from 'html-react-parser'\nimport {ImageSwiper, SizeTable} from '../components/Products/index'\n\nconst useStyle = makeStyles((theme) => ({\n    sliderBox: {\n      [theme.breakpoints.down('sm')]: {\n          margin: '0 auto 24px auto',\n          height: 320,\n          width: 320\n      },\n      [theme.breakpoints.up('sm')]: {\n        margin: '0 auto',\n        height: 400,\n        width: 400\n      }\n    },\n    detail: {\n      textAlign: 'left',\n      [theme.breakpoints.down('sm')]: {\n        margin: '0 auto 16px auto',\n        height: 'auto',\n        width: 320\n      },\n      [theme.breakpoints.up('sm')]: {\n        margin: '0 auto',\n        height: 'auto',\n        width: 400\n      }\n    },\n    price: {\n      fontSize: 36\n    }\n}))\n\n//改行コードをReactの中で表示させるための処理\nconst returnCodeToBr = (text) => {\n  if(text === \"\") {\n    return text\n  } else {\n    // /\\r?\\n/gは改行コードを表している \n    return HTMLReactParser(text.replace(/\\r?\\n/g, '<br>'))\n  }\n}\n\nconst ProductDetail = () => {\n  const classes = useStyle()\n  const selector = useSelector((state) => state);\n  const path = selector.router.location.pathname;\n  const id = path.split(\"/product/\")[1];\n\n  const [product, setProduct] = useState(null);\n\n  useEffect(() => {\n    db.collection(\"products\")\n      .doc(id)\n      .get()\n      .then((doc) => {\n        const data = doc.data();\n        setProduct(data);\n      });\n  }, []);\n\n  return (\n    <section className=\"c-section-wrapin\">\n        {product && (\n            <div className=\"p-grid__row\">\n                <div className={classes.sliderBox}>\n                    <ImageSwiper images={product.images} />\n                </div>\n                <div className={classes.detail}>\n                    <h2 className=\"u-text__headline\">{product.name}</h2>\n                    <p className={classes.price}>{product.price.toLocaleString()}</p>\n                    <div className=\"module-spacer--small\"></div>\n                    <SizeTable sizes={product.sizes} />\n                    <div className=\"module-spacer--small\"></div>\n                    <p>{returnCodeToBr(product.description)}</p>\n                </div>\n            </div>\n        )}\n    </section>\n  )\n};\n\nexport default ProductDetail;\n","/Users/tamakikyou/proguraming/ec.app/src/components/Products/ImageSwiper.jsx",["179"],"import React, {useState} from 'react'\nimport Swiper from 'react-id-swiper'\nimport NoImage from '../../assets/img/src/no_image.png'\nimport 'swiper/css/swiper.css'\n\nconst ImageSwiper = (props) => {\n    const [params] = useState({\n      pagintation: {\n          el: '.swiper-pagination',\n          type: 'bullets',\n          clickable: true,\n          dynamicBullets: true\n      },\n      navigation: {\n        nextEl: '.swiper-button-next',\n        prevEl: '.swiper-button-prev'\n      },\n      loop: true\n    })\n\n    const images = props.images\n\n    return(\n      <Swiper {...params} >\n        {images.length === 0 ? (\n          <div className=\"p-media__thumb\">\n              <img src={NoImage} alt=\"no image\" />\n          </div>\n        ) : (\n            images.map(image => (\n              <div className=\"p-media__thumb\">\n                  <img src={image.path} alt=\"商品画像\" />\n              </div>\n            ))\n        )}\n      </Swiper>\n    )\n}\n\nexport default ImageSwiper","/Users/tamakikyou/proguraming/ec.app/src/components/Products/SizeTable.jsx",[],"/Users/tamakikyou/proguraming/ec.app/src/components/Header/index.js",[],"/Users/tamakikyou/proguraming/ec.app/src/components/Header/Header.jsx",[],"/Users/tamakikyou/proguraming/ec.app/src/components/Header/HeaderMenus.jsx",[],"/Users/tamakikyou/proguraming/ec.app/src/components/Header/ClosableDrawer.jsx",[],{"ruleId":"180","replacedBy":"181"},{"ruleId":"182","replacedBy":"183"},{"ruleId":"184","severity":1,"message":"185","line":14,"column":6,"nodeType":"186","endLine":14,"endColumn":8,"suggestions":"187"},{"ruleId":"188","severity":1,"message":"189","line":1,"column":38,"nodeType":"190","messageId":"191","endLine":1,"endColumn":49},{"ruleId":"184","severity":1,"message":"192","line":65,"column":6,"nodeType":"186","endLine":65,"endColumn":8,"suggestions":"193"},{"ruleId":"194","severity":1,"message":"195","line":27,"column":15,"nodeType":"196","endLine":27,"endColumn":51},"no-native-reassign",["197"],"no-negated-in-lhs",["198"],"r